  },
  "homepage": "https://github.com/VincentMenzel/LinkedLists#readme",
  "devDependencies": {
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "@types/node": "^14.14.19",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.1"
  }
